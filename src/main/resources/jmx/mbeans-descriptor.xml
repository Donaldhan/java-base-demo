<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mbeans-descriptors PUBLIC  
 "-//Apache Software Foundation//DTD Model MBeans Configuration File"  
 "http://jakarta.apache.org/commons/dtds/mbeans-descriptors.dtd">  
  
<mbeans-descriptors>  
    <mbean name="runStatus" className="org.jmx.mbean.status.RunStatusMBean"  
        type="org.jmx.mbean.status.RunStatus">  
        <attribute name="applicationName" type="java.lang.String" />  
        <attribute name="memerySize" type="java.lang.Long" />
        <attribute name="coreCpuProssors" type="java.lang.Integer" />
        <attribute name="statusInfo" type="java.lang.String" writeable="false"/>  
        <operation name="printStatusInfo" impact="ACTION" returnType="void"/>  
        <!-- 
        <operation name="StatusInfo" impact="INFO" returnType="java.lang.String">  
            <parameter name="statusType" type="java.lang.String" />  
        </operation>   
        -->
    </mbean>  
</mbeans-descriptors> 